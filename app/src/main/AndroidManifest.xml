<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ado.edu.itla.taskapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".vista.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vista.CategoriaActivity" />
        <activity android:name=".vista.CategoriaListaActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".vista.RegistroActivity" />
        <activity android:name=".vista.TareaUsuarioNormalActivity" />
        <activity android:name=".vista.TareaAsignarActivity" />
        <activity android:name=".vista.TareaDetalleActivity" />
        <activity android:name=".vista.TareaUsuarioTecnicoActivity"></activity>
    </application>

</manifest>